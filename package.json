{
    "name": "eleventy-website",
    "version": "1.0.0",
    "description": "eleventy website for 120",
    "main": "index.js",
    "scripts": {
        "start": "npm-run-all --parallel dev:*",
        "build": "run-s prod:*",
        "dev:eleventy": "eleventy --serve --output=./docs",
        "dev:parcel": "parcel watch ./_assets/js/main.js --out-dir ./docs/assets",
        "prod:eleventy": "eleventy --output=./docs",
        "prod:parcel": "parcel build ./_assets/js/main.js --out-dir ./docs/assets"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/guidotorresmx/eleventy-website.git"
    },
    "keywords": [
        "eleventy",
        "blog"
    ],
    "author": "Marco Guido",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/guidotorresmx/eleventy-website/issues"
    },
    "homepage": "https://github.com/guidotorresmx/eleventy-website#readme",
    "devDependencies": {
        "@11ty/eleventy": "^1.0.0",
        "npm-run-all": "^4.1.5",
        "sass": "^1.49.9"
    },
    "dependencies": {
        "bootstrap": "^5.1.3",
        "eleventy-plugin-toc": "^1.1.5",
        "jquery": "^3.6.0",
        "markdown-it-anchor": "^8.4.1",
        "popper.js": "^1.16.1"
    }
}